# Environment variables for backend
# Copy this file to .env and fill in your actual values

# Database Configuration
# Replace with your actual database connection string
DB_HOST=localhost
DB_PORT=5432
DB_NAME=olp_db
DB_USER=your_username
DB_PASSWORD=your_password

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
SECRET_KEY=your_secret_key

# FIWARE Orion-LD Configuration
ORION_LD_URL=http://fiware-orionld:1026/ngsi-ld/v1
CONTEXT_URL=http://context/datamodels.context-ngsi.jsonld

# Logging
LOG_LEVEL=INFO

# === SUMO RL Configuration ===
# URL of Orion-LD broker for SUMO RL
ORION_URL=http://localhost:1026/ngsi-ld/v1

# Traffic Light ID from SUMO (Nga4ThuDuc scenario)
TLS_ID=4066470692

# Number of traffic light phases
NUM_PHASES=2

# Path to trained DQN model (relative to backend root or absolute)
MODEL_PATH=dqn_model.keras

